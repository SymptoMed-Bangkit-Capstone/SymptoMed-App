<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_host"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.uberalles.symptomed.ui.auth.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_splashFragment_to_nameFragment"
            app:destination="@id/nameFragment" />
        <action
            android:id="@+id/action_splashFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_splashFragment_to_genderFragment"
            app:destination="@id/genderFragment" />
        <action
            android:id="@+id/action_splashFragment_to_ageFragment"
            app:destination="@id/ageFragment" />
    </fragment>
    <fragment
        android:id="@+id/nameFragment"
        android:name="com.uberalles.symptomed.ui.intro.NameFragment"
        android:label="fragment_name"
        tools:layout="@layout/fragment_name" >
        <action
            android:id="@+id/action_nameFragment_to_genderFragment"
            app:destination="@id/genderFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/ageFragment"
        android:name="com.uberalles.symptomed.ui.intro.AgeFragment"
        android:label="fragment_age"
        tools:layout="@layout/fragment_age" >
        <action
            android:id="@+id/action_ageFragment_to_mainActivity"
            app:destination="@id/mainActivity"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainActivity"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/genderFragment"
        android:name="com.uberalles.symptomed.ui.intro.GenderFragment"
        android:label="fragment_gender"
        tools:layout="@layout/fragment_gender" >
        <action
            android:id="@+id/action_genderFragment_to_ageFragment"
            app:destination="@id/ageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.uberalles.symptomed.ui.main.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.uberalles.symptomed.ui.auth.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_signInFragment_to_nameFragment"
            app:destination="@id/nameFragment" />
        <action
            android:id="@+id/action_signInFragment_to_genderFragment"
            app:destination="@id/genderFragment" />
        <action
            android:id="@+id/action_signInFragment_to_ageFragment"
            app:destination="@id/ageFragment" />
        <action
            android:id="@+id/action_signInFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.uberalles.symptomed.ui.auth.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:popUpTo="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/offlineResultFragment"
        android:name="com.uberalles.symptomed.ui.result.OfflineResultFragment"
        android:label="fragment_offline_result"
        tools:layout="@layout/fragment_offline_result" >
        <action
            android:id="@+id/action_offlineResultFragment_to_offlineSymptomFragment"
            app:destination="@id/offlineSymptomFragment" />
    </fragment>
    <fragment
        android:id="@+id/offlineSymptomFragment"
        android:name="com.uberalles.symptomed.ui.main.prediction.OfflineSymptomFragment"
        android:label="fragment_offline_symptom"
        tools:layout="@layout/fragment_offline_symptom" >
        <action
            android:id="@+id/action_offlineSymptomFragment_to_offlineResultFragment"
            app:destination="@id/offlineResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/onlineSymptomFragment"
        android:name="com.uberalles.symptomed.ui.main.prediction.OnlineSymptomFragment"
        android:label="fragment_online_symptom"
        tools:layout="@layout/fragment_online_symptom" >
        <action
            android:id="@+id/action_onlineSymptomFragment_to_onlineResultFragment"
            app:destination="@id/onlineResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/onlineResultFragment"
        android:name="com.uberalles.symptomed.ui.result.OnlineResultFragment"
        android:label="fragment_online_result"
        tools:layout="@layout/fragment_online_result" >
        <action
            android:id="@+id/action_onlineResultFragment_to_onlineSymptomFragment"
            app:destination="@id/onlineSymptomFragment" />
    </fragment>
</navigation>